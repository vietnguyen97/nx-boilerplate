{
  "name": "@my-workspace/shared-utils",
  "version": "0.0.1",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "scripts": {
    "version": "nx run shared-utils:version",
    "prebuild": "yarn version",
    "build": "nx build shared-utils",
    "build:versioned": "node scripts/build-versioned.js"
  },
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "options": {
        "outputPath": "dist/shared-utils",
        "configFile": "shared-utils/vite.config.ts"
      }
    }
  }
}
